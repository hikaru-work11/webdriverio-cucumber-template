{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ES2022",
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "types": [
      "node",
      "@wdio/globals/types",
      "@wdio/cucumber-framework",
      "expect-webdriverio"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
